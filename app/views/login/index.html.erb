    <% if usuario_signed_in? %>
        <h3>Usuário</h3>
        <%= link_to "Deslogar", destroy_usuario_session_path, method: :delete, data: { turbo_method: :delete }, class: "btn btn-dark" %>
        <%= link_to "Editar login", edit_usuario_registration_path, class: "btn btn-dark" %>
        <h3>Página inicial</h3>
        <%= link_to "Página inicial", usuario_path, class: "btn btn-dark" %>
    <% elsif !admin_signed_in? %>
        <h3>Usuário</h3>
        <%= link_to "Registrar", new_usuario_registration_path, class: "btn btn-dark" %>
        <%= link_to "Logar", new_usuario_session_path, class: "btn btn-dark" %>
    <% end %>

    <% if admin_signed_in? %>
        <h3>Admin</h3>
        <%= link_to "Deslogar", destroy_admin_session_path, method: :delete, data: { turbo_method: :delete }, class: "btn btn-dark" %>
        <%= link_to "Editar login", edit_admin_registration_path, class: "btn btn-dark" %>
        <h3>Página inicial</h3>
        <%= link_to "Página inicial", admin_path, class: "btn btn-dark" %>
        <% elsif !usuario_signed_in? %>
        <h3>Admin</h3>
        <%= link_to "Registrar", new_admin_registration_path, class: "btn btn-dark" %>
        <%= link_to "Logar", new_admin_session_path, class: "btn btn-dark" %>
    <% end %>