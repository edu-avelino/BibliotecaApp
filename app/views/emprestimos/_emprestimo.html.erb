<div id="<%= dom_id emprestimo %>">
<p>
    <strong>Nome do aluno:</strong>
    <%= @alunos[emprestimo.id_aluno]&.nome || "Aluno não encontrado" %>
  </p>

  <p>
    <strong>Nome do livro:</strong>
    <%= @livros[emprestimo.id_livro]&.titulo || "Livro não encontrado" %>
  </p>

  <p>
    <strong>Data do empréstimo:</strong>
    <%= emprestimo.data_empr %>
  </p>

  <p>
    <strong>Data de devolução:</strong>
    <%= emprestimo.data_devo %>
  </p>

  <p>
    <strong>Status - Foi devolvido?:</strong>
    <%= emprestimo.status%>
  </p>

</div>
