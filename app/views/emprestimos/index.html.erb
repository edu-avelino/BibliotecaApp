<p style="color: green"><%= notice %></p>

<%= paginate @emprestimos, theme: 'bootstrap-5' %>

<% content_for :title, "Emprestimos" %>

<h1>Emprestimos</h1>

<div id="emprestimos">
  <% @emprestimos.each do |emprestimo| %>
    <%= render emprestimo %>
    <p>
      <%= link_to "Mostrar esse emprestimo", emprestimo %>
    </p>
  <% end %>
</div>

<%= link_to "Novo emprestimo", new_emprestimo_path %>
